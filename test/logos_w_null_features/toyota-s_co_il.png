                                case 'tel':
                                case 'email':
                                case 'url':
                                    element.value = value;
                                    break;
                                case 'checkbox':
                                    element.checked = parseInt(value);
                                    break;
                            }
                            break;
                        case 'select':
                        case 'textarea':
                            element.value = value;
                            break;
                    }
                }
