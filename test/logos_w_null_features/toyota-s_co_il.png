    function phoneValidation(element) {
        element.pattern = '((05|07)[0-9]{8}|(?!05|07)[0-9]{9})';
        element.setAttribute("oninvalid", "this.setCustomValidity(this.value == '' ? 'This field is required' : 'This field is invalid')");
        element.setAttribute("onchange", "try{setCustomValidity('')}catch(e){}");
    }
    function setLookUpValue(element, value) {
        var div = element.nextSibling;
        var ul = div.children[0];
        if (!ul.children[0].classList.contains('ui-loading-item')) {
            debugger;
            var li = ul.querySelectorAll('[data-value="' + value + '"]')[0];
            if (li != null) {
                SetLookUpDivForScrolling(div, false);
                li.classList.add("ui-state-active");
                element.value = li.children[0].innerHTML;
            }
            else {
                SetLookUpDivForScrolling(div, true);
                ul.scrollTop = ul.scrollHeight;
                setTimeout(function () { setLookUpValue(element, value); }, 100);
            }
        }
        else
            setTimeout(function () { setLookUpValue(element, value); }, 100);
    }
    function getLookUp(element) {
        debugger;
        var div = element.nextSibling;
        var ul = div.children[0];
        var li = ul.querySelectorAll('[class="ui-menu-item ui-state-active"]')[0];
        if (li != null) {
            var lookUp = new Object();
            lookUp.value = li.getAttribute('data-value');
            lookUp.text = li.children[0].innerText;
            return lookUp;
        }
        else {
            return null;
        }
    }
    function SetLookUpDivForScrolling(div, scrollable) {
        div.style.visibility = (scrollable ? 'hidden' : 'visible');
        div.style.display = (scrollable ? 'block' : 'none');
    }
    function getSearchParameter(name) {
        var half = location.search.split('&' + name + '=')[1];
        if (!half) half = location.search.split('?' + name + '=')[1];
        return half !== undefined ? decodeURIComponent(half.split('&')[0]) : null;
    }
